<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="FileConnectorInstance"
		class="com.google.enterprise.connector.file.FileConnector">
		
		<property name="login" value="${login}" />
		<property name="password" value="${password}" />
		<property name="credTag" value="${credTag}" />
		<property name="objectStoreName" value="${objectStoreName}" />
		<property name="pathToWcmApiConfig" value="C:\_dev\google\connector\connector-file\projects\third_party\WcmApiConfig.properties" />
		<property name="displayUrl" value="${displayUrl}" />
		<property name="objectFactory" value="com.google.enterprise.connector.file.filejavawrap.FnObjectFactory" />
		<property name="isPublic" value="${isPublic}" />
	</bean>
</beans>